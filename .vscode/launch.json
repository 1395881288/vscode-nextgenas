{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}/target/vscode-nextgenas-bin/vscode-nextgenas"
			],
			"stopOnEntry": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/target/vscode-nextgenas-bin/vscode-nextgenas",
			"preLaunchTask": "build-debug"
		},
		{
			"name": "Launch Tests",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"${workspaceRoot}/vscode-extension/src/test/fixtures",
				"--extensionDevelopmentPath=${workspaceRoot}/target/vscode-nextgenas-bin/vscode-nextgenas",
				"--extensionTestsPath=${workspaceRoot}/vscode-extension/target/test"
			],
			"stopOnEntry": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/vscode-extension/target/test",
			"preLaunchTask": "build-debug"
		}
	]
}